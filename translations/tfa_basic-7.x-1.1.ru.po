msgstr "tfa_basic"

#: tfa_basic.pages.inc:342
msgid "TFA disabled for user @name UID !uid"
msgstr "TFA отключен для пользователя @name UID !uid"

#: tfa_basic.pages.inc:637
msgid "TFA SMS disabled for user @name UID !uid"
msgstr "TFA по SMS отключен для пользователя @name UID !uid"

#: tfa_basic.pages.inc:698
msgid "TFA enabled for user @name UID !uid"
msgstr "TFA включен для пользователя @name UID !uid"

